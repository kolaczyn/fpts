{
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "plugins": ["jest"],
  "rules": {
    "no-var": "error",
    "@typescript-eslint/no-unused-vars": "error",
    "semi": "off",
    "sort-imports": "error",
    "no-unused-vars": "off",
    "no-console": "warn",
    "camelcase": "error",
    "prefer-const": "error"
  }
}
